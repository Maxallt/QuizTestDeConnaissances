<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane stylesheets="@myApplication.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="interfacesQuizTestConnaissances.PopUpCategorieInexistanteController">
   <children>
      <Pane minHeight="200.0" minWidth="300.0" styleClass="Fenetre">
         <children>
            <Label layoutX="50.0" layoutY="-10.0" prefHeight="120.0" prefWidth="215.0" text="La catégorie suivante &#10;n'existe pas :" textAlignment="CENTER" textFill="#cc0000">
               <font>
                  <Font name="Comic Sans MS" size="20.0" />
               </font>
            </Label>
            <Button fx:id="buttonCreer" layoutX="213.0" layoutY="144.0" mnemonicParsing="false" onAction="#confirmationCreationCategorie" text="Créer" />
            <Button fx:id="buttonAnnuler" layoutX="30.0" layoutY="144.0" mnemonicParsing="false" onAction="#annuler" text="Abandonner la question" />
            <Label fx:id="nomCat" layoutX="30.0" layoutY="90.0" prefHeight="40.0" prefWidth="228.0" text="catACréer" textAlignment="CENTER">
               <font>
                  <Font name="Comic Sans MS" size="14.0" />
               </font>
            </Label>
         </children>
      </Pane>
   </children>
</AnchorPane>
