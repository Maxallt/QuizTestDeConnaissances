<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane stylesheets="@myApplication.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="interfacesQuizTestConnaissances.PopUpListeFaibleController">
   <children>
      <Pane minHeight="200.0" minWidth="300.0" styleClass="Fenetre">
         <children>
            <Label fx:id="boiteDialogue" layoutX="8.0" layoutY="14.0" prefHeight="120.0" prefWidth="291.0" text="Pas assez de questions&#10;Continuer ?&#10;Oui -&gt; cliquer sur valider" textAlignment="CENTER" textFill="#cc0000">
